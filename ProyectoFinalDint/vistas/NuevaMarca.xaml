<Window x:Class="ProyectoFinalDint.vistas.NuevaMarca"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoFinalDint.vistas"
        xmlns:inputLayout="clr-namespace:Syncfusion.UI.Xaml.TextInputLayout;assembly=Syncfusion.SfTextInputLayout.WPF"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        ShowInTaskbar="False"
        Title="Añadir nueva marca" Height="150" Width="400">
    <StackPanel Margin="5">
        <inputLayout:SfTextInputLayout Hint="Nueva Marca">
            <TextBox/>
        </inputLayout:SfTextInputLayout>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
        <Button Margin="20,0" Width="100" IsDefault="True" Click="AceptarButton_Click">Aceptar</Button>
            <Button Width="100" IsCancel="True">Cancelar</Button>
        </StackPanel>
    </StackPanel>
</Window>
